# Copy to .env and fill in your values.

# JWT secret for login tokens (use a long random string in production)
JWT_SECRET=your-secret-key

# MongoDB connection string (from MongoDB Atlas or any MongoDB host)
# Example: mongodb+srv://user:password@cluster.mongodb.net/codelab?retryWrites=true&w=majority
MONGODB_URI=mongodb+srv://...

# Optional: use DATABASE_URL instead of MONGODB_URI (same format as above)
# DATABASE_URL=mongodb+srv://...
